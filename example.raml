  /select.json:
    get:
      description: Seleciona o perito que tiver cumprido a menor parcela de laudos. Em outras palavras, aquele que menos tiver feito laudo é escolhido. Em caso de empate, a escolha é feita de forma aleatória.
      responses:
        200:
          description: Sucesso.
          body:
            application/json:
              example: |
                {
                  "success": true,
                  "status": 200
                }
        400:
          description: Você precisa de autorização para realizar essa requisição. 
          body:
            application/json:
              example: |
                {
                  "success": false,
                  "status": 400
                }     
  
  /add.json:
      post:
        description: Esse endpoint é responsável por adicionar uma nova requisição no sistema.
        queryParameters:
          descricao:
            displayName: descricao
            type: string
            description: Descrição escrita da requisição e demais informações pertinentes.
            example: "Requisição de perícia de veículos"
            required: false
          data_documento:
            displayName: data_documento
            type: date-only
            description: Data do documento que criou a nova requisição.
            example: "2020-02-16"
            required: false
          n_documento:
            displayName: n_documento
            type: string
            description: Número do documento que foi recebido e deu origem a uma nova requisição.
            example: "578/2012"
            required: false
          data_realizacao_pericia:
            displayName: data_realizacao_pericia
            type: date-only
            description: Data em que foi realizada a perícia.
            example: "2019-09-30"
            required: false
          data_recebimento:
            displayName: data_recebimento
            type: date-only
            description: Data de recebimento da requisição.
            example: "2013-01-31"
            required: false
          escrivao:
            displayName: escrivao
            type: string
            description: Escrivão responsável pelo recebimento da requisição.
            example: "Carla Marques"
            required: false
          delegacia:
            displayName: delegacia
            type: string
            description: Delegacia de onde se originou a requisição.
            example: "15ª DP Paranaíba"
            required: false
          autoridade_requisitante:
            displayName: autoridade_requisitante
            type: string
            description: Nome da autoridade que fez a requisição de perícia.
            example: "João Paraná"
            required: false
          logradouro:
            displayName: logradouro
            type: string
            description: Nome do logradouro onde ocorreu o crime.
            example: "Mato Grosso"
            required: false
          tipo_logradouro:
            displayName: tipo_logradouro
            type: string
            description: Tipo do logradouro onde ocorreu o crime.
            example: "Avenida"
            required: false
          nmr_logradouro:
            displayName: nmr_logradouro
            type: string
            description: Número do logradouro onde ocorreu o crime.
            example: "3332"
            required: false
          bairro:
            displayName: bairro
            type: string
            description: Nome do bairro onde ocorreu o crime.
            example: "Centro"
            required: false
          cidade:
            displayName: cidade
            type: string
            description: Nome da cidade onde ocorreu o crime.
            example: "Paranaíba"
            required: false
          n_laudos_expedidos:
            displayName: n_laudos_expedidos
            type: string
            description: Número de laudos que foram originados da requisição.
            example: "3"
            required: false
          cargo:
            displayName: cargo
            type: string
            description: Cargo do remetente da requisição.
            example: "Delegado"
            required: false
          observacoes:
            displayName: observacoes
            type: string
            description: Demais informações sobre a requisição.
            example: "Perícia urgente"
            required: false
          n_oficio:
            displayName: n_oficio
            type: string
            description: Número do ofício que originou a requisição.
            example: "22/2019"
            required: false
          tipo_pericia:
            displayName: tipo_pericia
            type: string
            description: O tipo de perícia a ser realizada pelo perito responsável.
            example: "Balística"
            required: false
          exame_pericia:
            displayName: exame_pericia
            type: string
            description: O exame relacionado ao objetivo da perícia.
            example: "Exame in loco"
            required: false
          n_bo:
            displayName: n_bo
            type: string
            description: Número do boletim de ocorrência relacionado com a requisição.
            example: "123/2016"
            required: false
          n_ip:
            displayName: n_ip
            type: string
            description: Número do Inquérito Policial relacionado com a requisição.
            example: "23/2016"
            required: false
          outros_proc:
            displayName: outros_proc
            type: string
            description: Outros processos relacionados com a requisição.
            example: "Ofício n° 215 do 11º DP de Cassilândia"
            required: false
          nome_vitima:
            displayName: nome_vitima
            type: string
            description: Nome da vítima relacionada com a requisição.
            example: "Claudio Castro"
            required: false
        responses:
          200:
            description: O laudo foi adicionado com sucessso.
            body:
              application/json:
                example: |
                  {
                    "data": {
                      "id": 59,
                      "user_id": 7,
                      "deliver_date": "2020/05/15",
                      "receiver": "Antonio Silva",
                      "status": "Entregue"
                    },
                    "success": true,
                    "status": 200
                    }
          400:
            description: Erro ao adicionar laudo.
            body:
              application/json:
                example: |
                    {
                    "data": {
                      "id": 66,
                      "user_id": 8,
                      "deliver_date": "2020/05/15",
                      "receiver": "Antonio Silva",
                      "status": "Entregue"
                    },
                    "success": false,
                    "status": 400
                    }
                        
    
