#%RAML 1.0  
title: Gerenciador Pericial
baseUri: https://191.252.202.56/fabrica/p05-ger-pericial/
version: v1.6

/account:
  /verifyToken.json:
    post:
      description: Esse endpoint é responsável por realizar uma autenticação do token do usuário para liberar as permissões de acesso e modificação deste usuário. Atua diversas vezes no sistema, verificando se o token existe, não existe, é inválido, ou está expirado.
      queryParameters:
        id:
          displayName: id
          type: number
          description: O identificador do usuário a ter seu token verificado.
          example: 21
          required: true
        tokenCode:
          displayName: tokenCode
          type: string
          description: Código do token autenticador do usuário.
          example: "0x e3b0cf991b7852b855342b516234fca213457"
          required: true
      responses:
        200:
          description: Token válido.
          body:
            application/json:
              example: |
                {
                  "data": {
                    "id": 12,
                    "tokenCode": "0x e3b0cf991b7852b855342b516234fca213457"
                  },
                  "success": true,
                  "status": 200
                  }
        400:
          description: Token expirado, inválido ou inexistente.
          body:
            application/json:
              example: |
                  {
                    "data": {
                      "id": 12,
                      "tokenCode": "0x e3b0cf991b7852b855342b516234fca213457"
                    },
                  "success": false,
                  "status": 400
                  }
  /changePass.json:
    post:
      description: Esse endpoint é responsável por trocar a senha do usuário.
      queryParameters:
        id:
          displayName: id
          type: number
          description: O identificador do usuário que vai mudar sua senha.
          example: 25
          required: true
        password:
          displayName: password
          type: string
          description: A nova senha do usuário será utilizada para realização do login no sistema.
          example: "12345678"
          required: true
      responses:
        200:
          description: Senha alterada com sucesso.
          body:
            application/json:
              example: |
                {
                  "data": {
                    "id": 25,
                    "password": "12345678"
                  },
                  "success": true,
                  "status": 200
                }
        400:
          description: A senha não foi alterada. Por favor, tente novamente. 
          body:
            application/json:
              example: |
                {
                  "data": {
                    "id": 25,
                    "password": "12345678"
                  },
                  "success": false,
                  "status": 400
                }     
  /lostAccount.json:
    post:
      description: Esse endpoint é responsável por recuperar a conta de um usuário que esqueceu sua senha.
      queryParameters:
        email:
          displayName: email
          type: string
          description: O email do usuário será utilizado para realização da recuperação de conta.
          example: "joao.sousa@email.com.br"
          required: true
      responses:
        200:
          description: Email de recuperação de senha enviado com sucesso.
          body:
            application/json:
              example: |
                {
                  "data": {
                    "email": "joao.sousa@email.com.br"
                  },
                  "success": true,
                  "status": 200
                }
        400:
          description: Erro. Email de recuperação de senha não enviado.
          body:
            application/json:
              example: |
                {
                  "data": {
                    "email": "joao.sousa@email.com.br"
                  },
                  "success": false,
                  "status": 400
                }
 
  
  /confirmAccount.json:
    post:
      description: Esse endpoint é responsável por confirmar a conta de um novo usuário.
      queryParameters:
        id:
          displayName: id
          type: number
          description: O identificador do usuário que vai ter sua conta confirmada.
          example: 15
          required: true
      responses:
        200:
          description: A conta foi confirmada com sucesso.
          body:
            application/json:
              example: |
                {
                  "data": {
                    "id": 22
                  },
                  "success": true,
                  "status": 200
                }
        400:
          description: Conta não confirmada. Por Favor, tente novamente.
          body:
            application/json:
              example: |
                {
                  "data": {
                    "id": 22
                  },
                  "success": false,
                  "status": 400
                }
 
  
  /resendEmail.json:
    post:
      description: Esse endpoint é responsável por reenviar o email de confirmação da conta de um novo usuário.
      queryParameters:
        id:
          displayName: id
          type: number
          description: O identificador do usuário que irá receber outro email de confirmação de conta.
          example: 57
          required: true
      responses:
        200:
          description: O email foi enviado com sucesso.
          body:
            application/json:
              example: |
                {
                  "data": {
                    "id": 57
                  },
                  "success": true,
                  "status": 200
                }
        400:
          description: Erro ao enviar email.
          body:
            application/json:
              example: |
                {
                  "data": {
                    "id": 57
                  },
                  "success": false,
                  "status": 400
                }
 
  
  

/login.json:
  post:
    description: Esse endpoint é responsável por realizar a autenticação (log in) do usuário no sistema, permitindo sua utilização.
    queryParameters:
        email:
          displayName: email
          type: string
          description: O email do usuário que quer entrar no sistema.
          example: "maria.da.silva@email.com"
          required: true
        password:
          displayName: password
          type: string
          description: A senha referente a esse usuário.
          example: "12345678"
          required: true
    responses:
        200:
          description: Token válido.
          body:
            application/json:
              example: |
                {
                  "data": {
                    "email": "maria.da.silva@email.com",
                    "password": "123456789"
                  },
                  "success": true,
                  "status": 200
                  }
        400:
          description: Usuário ou senha inválidos.
          body:
            application/json:
              example: |
                  {
                    "data": {
                      "email": "maria.da.silva@email.com",
                      "password": "123467892"
                    },
                  "success": false,
                  "status": 400
                  }
  
  
/reports:
   /view.json:
    get:
      description: Esse endpoint retorna todos os laudos registrados no sistema.
      responses:
        200:
          description: Sucesso.
          body:
            application/json:
              example: |
                {
                  "success": true,
                  "status": 200
                }
        400:
          description: Você precisa de autorização para realizar essa requisição. 
          body:
            application/json:
              example: |
                {
                  "success": false,
                  "status": 400
                }     
  
   /add.json:
      post:
        description: Esse endpoint é responsável por adicionar um novo laudo ao sistema.
        queryParameters:
          report_id:
            displayName: report_id
            type: number
            description: Número de ordem do novo laudo.
            example: 132
            required: true
          user_id:
            displayName: user_id
            type: number
            description: O perito responsável pela confecção do laudo.
            example: 9
            required: true
          request_id:
            displayName: request_id
            type: number
            description: O perito responsável pela confecção do laudo.
            example: 92
            required: true
          deliver_date:
            displayName: deliver_date
            type: date-only
            description: Data da entrega do laudo.
            example: "2020-05-15"
            required: false
          receiver:
            displayName: receiver
            type: string
            description: Servidor que recebeu o laudo pronto.
            example: "Antônio Sousa"
            required: false
          status:
            displayName: status
            type: string
            description: Situação atual do laudo.
            example: "Entregue"
            required: false
        responses:
          200:
            description: O laudo foi adicionado com sucessso.
            body:
              application/json:
                example: |
                  {
                    "data": {
                      "id": 433,
                      "user_id": 123,
                      "request_id": 55,
                      "deliver_date": "2020/05/15",
                      "receiver": "Antonio Silva",
                      "status": "Entregue"
                    },
                    "success": true,
                    "status": 200
                    }
          400:
            description: Erro ao adicionar laudo.
            body:
              application/json:
                example: |
                    {
                    "data": {
                      "id": 433,
                      "user_id": 123,
                      "request_id": 55,
                      "deliver_date": "2020/05/15",
                      "receiver": "Antonio Silva",
                      "status": "Entregue"
                    },
                    "success": false,
                    "status": 400
                    }
                        
    
